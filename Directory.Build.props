<?xml version="1.0" encoding="utf-8"?>
<!-- This file contains project properties used by the build. -->
<Project>
	<PropertyGroup>
		<ImportBSMTTargets>True</ImportBSMTTargets>
		<BSMTProjectType>BSIPA</BSMTProjectType>
	</PropertyGroup>
	<ItemGroup>
		<OutputCopy Include="$(TargetPath)" OutputPath="Plugins\$(TargetName).dll" />
		<OutputCopy Include="$(ProjectDir)$(OutputPath)EnhancedStreamChat.pdb" OutputPath="Plugins\$(TargetName).pdb" />
		<OutputCopy Include="FontAssets\mplus-1c-medium sdf.assets" OutputPath="UserData\FontAssets\Main\mplus-1c-medium sdf.assets" />
	</ItemGroup>
</Project>